{
  "name": "pocketo",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev -p 9807",
    "build": "next build",
    "test": "jest",
    "test:ci": "jest --ci",
    "test:e2e": "playwright test",
    "build-profile": "next build --profile",
    "start": "next start -p 9807",
    "lint": "next lint",
    "type-check": "tsc --project tsconfig.json --pretty --noEmit",
    "tailwind-config-viewer": "tailwind-config-viewer -o",
    "size": "ANALYZE=true npm run build",
    "dependencies-square": "depcruise -v --ts-config tsconfig.json -x '(.stories.|.test.)' --include-only '^src' -T html -f dependencyGraph.html ./src",
    "dependencies": "depcruise -v --ts-config tsconfig.json -x '(.stories.|.test.)' --include-only '^src' --output-type dot . | dot -T svg > dependencygraph.svg",
    "dependencies-with-types": "depcruise -v --ts-config tsconfig.json -x '(.stories.|.test.)' --ts-pre-compilation-deps --include-only '^src' --output-type dot . | dot -T svg > dependencygraph.svg",
    "dependencies-with-nodeModules": "depcruise -v --ts-config tsconfig.json -x '(.stories.|.test.)' --include-only '^(node_modules|src)' --output-type dot . | dot -T svg > dependencygraph.svg",
    "prepare": "husky install",
    "production": "npm run build && npm run start",
    "production-profile": "npm run build-profile && npm run start",
    "storybook": "start-storybook -p 6006",
    "build-storybook": "build-storybook"
  },
  "dependencies": {
    "@react-icons/all-files": "^4.1.0",
    "@tool-belt/type-predicates": "^1.2.0",
    "clsx": "^1.1.1",
    "copy-text-to-clipboard": "^3.0.1",
    "daisyui": "^2.13.6",
    "next": "^12.1.4",
    "notistack": "3.0.0-alpha.2",
    "pocket-auth": "^1.2.4",
    "ramda": "^0.28.0",
    "react": "^17.0.2",
    "react-content-loader": "^6.2.0",
    "react-dom": "^17.0.2",
    "react-hook-form": "^7.29.0",
    "react-hotkeys-hook": "^3.4.4",
    "react-mentions": "^4.3.1",
    "react-query": "^3.34.19",
    "react-share": "^4.4.0",
    "react-tag-input": "^6.8.0",
    "react-virtuoso": "^2.8.6",
    "tailwindcss": "^3.0.23"
  },
  "devDependencies": {
    "@next/bundle-analyzer": "^12.1.4",
    "@next/eslint-plugin-next": "^12.1.4",
    "@playwright/test": "^1.20.2",
    "@storybook/addon-a11y": "^6.4.14",
    "@storybook/addon-actions": "^6.4.14",
    "@storybook/addon-docs": "^6.4.14",
    "@storybook/addon-essentials": "^6.4.14",
    "@storybook/addon-links": "^6.4.14",
    "@storybook/addon-postcss": "^2.0.0",
    "@storybook/addon-storysource": "6.4.14",
    "@storybook/builder-webpack5": "^6.4.14",
    "@storybook/manager-webpack5": "^6.4.14",
    "@storybook/react": "^6.4.14",
    "@testing-library/jest-dom": "^5.16.4",
    "@testing-library/react": "^12.1.2",
    "@types/eslint": "8.4.1",
    "@types/jest": "^27.4.1",
    "@types/ramda": "^0.28.7",
    "@types/react": "17.0.43",
    "@types/react-dom": "17.0.14",
    "@types/react-mentions": "^4.1.5",
    "@types/react-tag-input": "^6.1.3",
    "@types/rewire": "^2.5.28",
    "@typescript-eslint/parser": "^5.18.0",
    "autoprefixer": "^10.4.4",
    "css-loader": "^6.7.1",
    "dependency-cruiser": "^11.4.1",
    "eslint": "^8.12.0",
    "eslint-config-hardcore": "24.5.0",
    "eslint-config-next": "12.1.4",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-storybook": "^0.5.7",
    "eslint-plugin-tailwindcss": "^3.5.0",
    "eslint-plugin-testing-library": "5.2.1",
    "husky": "^7.0.4",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^27.5.1",
    "lint-staged": "^12.3.7",
    "postcss": "^8.4.12",
    "prettier": "2.6.2",
    "react-test-renderer": "^17.0.2",
    "sass": "^1.49.11",
    "sass-loader": "^12.6.0",
    "sharp": "^0.30.3",
    "storybook-addon-performance": "^0.16.1",
    "style-loader": "^3.3.1",
