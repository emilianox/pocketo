{
  "name": "pocketo",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev -p 9807",
    "build": "next build",
    "test": "jest",
    "build-profile": "next build --profile",
    "start": "next start -p 9807",
    "lint": "next lint",
    "type-check": "tsc --project tsconfig.json --pretty --noEmit",
    "tailwind-config-viewer": "tailwind-config-viewer -o",
    "size": "ANALYZE=true npm run build",
    "dependencies-square": "depcruise -v --ts-config tsconfig.json --include-only '^src' -T html -f dependencyGraph.html ./src",
    "dependencies": "depcruise -v --ts-config tsconfig.json --include-only '^src' --output-type dot . | dot -T svg > dependencygraph.svg",
    "dependencies-with-types": "depcruise -v --ts-config tsconfig.json --ts-pre-compilation-deps --include-only '^src' --output-type dot . | dot -T svg > dependencygraph.svg",
    "dependencies-with-nodeModules": "depcruise -v --ts-config tsconfig.json --include-only '^(node_modules|src)' --output-type dot . | dot -T svg > dependencygraph.svg",
    "prepare": "husky install",
    "production": "npm run build && npm run start",
    "production-profile": "npm run build-profile && npm run start",
    "storybook": "start-storybook -p 6006",
    "build-storybook": "build-storybook"
  },
  "dependencies": {
    "@react-icons/all-files": "^4.1.0",
    "@tool-belt/type-predicates": "^1.2.0",
    "clsx": "^1.1.1",
    "copy-text-to-clipboard": "^3.0.1",
    "daisyui": "^1.22.0",
    "next": "^12.0.7",
    "notistack": "^1.0.10",
    "pocket-auth": "^1.2.4",
    "ramda": "^0.27.2",
    "react": "^17.0.2",
    "react-content-loader": "^6.0.3",
    "react-dom": "^17.0.2",
    "react-hook-form": "^7.14.2",
    "react-hotkeys-hook": "^3.4.0",
    "react-mentions": "^4.3.0",
    "react-query": "^3.21.1",
    "react-share": "^4.4.0",
    "react-tag-input": "^6.7.3",
    "react-virtuoso": "^2.4.0",
    "tailwindcss": "^3.0.13"
  },
  "devDependencies": {
    "@babel/core": "^7.16.7",
    "@next/bundle-analyzer": "^12.0.7",
    "@storybook/addon-actions": "^6.5.0-alpha.14",
    "@storybook/addon-essentials": "^6.5.0-alpha.14",
    "@storybook/addon-links": "^6.5.0-alpha.14",
    "@storybook/addon-postcss": "^2.0.0",
    "@storybook/builder-webpack5": "^6.5.0-alpha.14",
    "@storybook/manager-webpack5": "^6.5.0-alpha.14",
    "@storybook/react": "^6.5.0-alpha.14",
    "@testing-library/jest-dom": "^5.14.1",
    "@testing-library/react": "^12.1.2",
    "@types/eslint": "8.2.1",
    "@types/jest": "^27.0.2",
    "@types/ramda": "^0.27.44",
    "@types/react": "17.0.38",
    "@types/react-dom": "17.0.11",
    "@types/react-mentions": "^4.1.3",
    "@types/react-tag-input": "^6.1.3",
    "@types/rewire": "^2.5.28",
    "@typescript-eslint/parser": "^5.8.1",
    "autoprefixer": "^10.4.1",
    "babel-jest": "^27.2.5",
    "babel-loader": "^8.2.3",
    "babel-plugin-console-source": "^2.0.5",
    "babel-plugin-ramda": "^2.0.0",
    "babel-plugin-typescript-to-proptypes": "^1.4.2",
    "dependency-cruiser": "^10.2.0",
    "eslint": "^8.6.0",
    "eslint-config-hardcore": "23.7.0",
    "eslint-config-next": "12.0.7",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-storybook": "^0.5.5",
    "eslint-plugin-tailwindcss": "^3.0.2",
    "husky": "^7.0.2",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^27.2.5",
    "lint-staged": "^11.1.2",
    "postcss": "^8.4.5",
    "prettier": "2.5.1",
    "react-test-renderer": "^17.0.2",
    "rewire": "^3.0.2",
    "sass": "^1.39.0",
    "sharp": "^0.29.0",
    "stylelint": "^13.13.1",
    "stylelint-config-recommended-scss": "^4.3.0",
    "stylelint-order": "^4.1.0",
    "stylelint-scss": "^3.20.1",
    "tailwind-config-viewer": "^1.6.3",
    "ts-essentials": "^8.1.0",
    "typescript": "4.5.4",
    "webpack": "^5.65.0"
  },
  "resolutions": {
    "webpack": "^5.65.0"
  }
}
