{
  "name": "pocketo",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "build-profile": "next build --profile",
    "start": "next start",
    "lint": "next lint",
    "type-check": "tsc --project tsconfig.json --pretty --noEmit",
    "tailwind-config-viewer": "tailwind-config-viewer -o",
    "size": "ANALYZE=true npm run build",
    "dependencies-square": "depcruise -v --ts-config tsconfig.json --include-only '^src' -T html -f dependencyGraph.html ./src",
    "dependencies": "depcruise -v --ts-config tsconfig.json --include-only '^src' --output-type dot . | dot -T svg > dependencygraph.svg",
    "dependencies-with-nodeModules": "depcruise -v --ts-config tsconfig.json --include-only '^(node_modules|src)' --output-type dot . | dot -T svg > dependencygraph.svg",
    "prepare": "husky install"
  },
  "dependencies": {
    "@tool-belt/type-predicates": "^1.1.0",
    "clsx": "^1.1.1",
    "copy-text-to-clipboard": "^3.0.1",
    "daisyui": "^1.14.0",
    "next": "11.1.2",
    "pocket-auth": "^1.2.4",
    "ramda": "^0.27.1",
    "react": "17.0.2",
    "react-dom": "17.0.2",
    "react-icons": "^4.2.0",
    "react-query": "^3.21.1",
    "react-share": "^4.4.0",
    "react-tag-input": "^6.7.3",
    "react-virtual": "^2.8.1",
    "sass": "^1.39.0",
    "tailwindcss": "^2.2.9"
  },
  "devDependencies": {
    "@next/bundle-analyzer": "^11.1.2",
    "@types/eslint": "7.28.0",
    "@types/ramda": "^0.27.44",
    "@types/react": "17.0.19",
    "@types/react-dom": "17.0.9",
    "@types/react-tag-input": "^6.1.3",
    "autoprefixer": "^10.3.4",
    "dependency-cruiser": "^10.2.0",
    "eslint": "^7.32.0",
    "eslint-config-hardcore": "^20.3.0",
    "eslint-config-next": "11.1.2",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-prettier": "^3.4.1",
    "eslint-plugin-tailwindcss": "^1.14.3",
    "husky": "^7.0.2",
    "lint-staged": "^11.1.2",
    "postcss": "^8.3.6",
    "prettier": "2.3.2",
    "sharp": "^0.29.0",
    "tailwind-config-viewer": "^1.6.2",
    "ts-essentials": "^8.1.0",
    "typescript": "4.4.2"
  }
}
